# Clerk Authentication
VITE_CLERK_PUBLISHABLE_KEY=pk_test_...
CLERK_PUBLISHABLE_KEY=pk_test_...
CLERK_SECRET_KEY=sk_test_...

# LLM Providers
OPENAI_API_KEY=sk-...
ANTHROPIC_API_KEY=sk-ant-...

# Default provider: "openai" | "anthropic"
AI_PROVIDER=anthropic

# Image Providers (optional - images disabled if not set)
UNSPLASH_ACCESS_KEY=
PEXELS_API_KEY=

# Google Maps Embed API (optional - falls back to basic embed if not set)
VITE_GOOGLE_MAPS_API_KEY=

# Image Bank (optional - S3 storage for image caching)
# Uses AWS_PROFILE from environment for credentials
AWS_REGION=us-west-2
S3_BUCKET=

# Logging: "debug" | "info" | "warn" | "error" (default: info)
LOG_LEVEL=info
